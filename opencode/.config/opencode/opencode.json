{
  "$schema": "https://opencode.ai/config.json",
  "keybinds": {
    "app_exit": "ctrl+c",
    "command_list": "ctrl+p",
    "editor_open": "ctrl+g",
    "history_next": "down",
    "history_previous": "up",
    "input_backspace": "backspace",
    "input_clear": "ctrl+l",
    "input_delete": "delete,ctrl+d",
    "input_delete_to_line_end": "ctrl+k",
    "input_delete_to_line_start": "ctrl+u",
    "input_delete_word_backward": "ctrl+w,alt+backspace",
    "input_delete_word_forward": "alt+d,alt+delete",
    "input_line_end": "ctrl+e,end",
    "input_line_home": "ctrl+a,home",
    "input_move_left": "left,ctrl+b",
    "input_move_right": "right,ctrl+f",
    "input_newline": "shift+return",
    "input_redo": "ctrl+shift+z,super+shift+z",
    "input_submit": "return",
    "input_undo": "ctrl+z,super+z",
    "input_word_backward": "alt+b,alt+left",
    "input_word_forward": "alt+f,alt+right",
    "messages_copy": "ctrl+shift+c",
    "messages_page_down": "pagedown",
    "messages_page_up": "pageup",
    "messages_redo": "none",
    "messages_undo": "ctrl+_",
    "model_list": "ctrl+m",
    "session_interrupt": "escape",
    "session_list": "ctrl+shift+l",
    "session_new": "ctrl+shift+n",
    "sidebar_toggle": "ctrl+b",
    "status_view": "ctrl+o",
    "tips_toggle": "ctrl+t"
  }
}
